<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:p="http://www.springframework.org/schema/p"

       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
          http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd
        ">
    <bean name= "mall" class="technosoft.Mall" autowire="byType">
        <property name="regNo" value ="123" />
        <property name="mallName" value ="Magneto" />
        <property name="city" value ="Indore" />
<!--        <property name= "govt" ref="govt"/>-->
    </bean>
    <bean name= "govt" class="technosoft.Govt">
        <property name="pmName" value ="Narendra Modi" />
        <property name="country" value ="Bharat" />
        <property name="rules" value ="Parameters" />
    </bean>
